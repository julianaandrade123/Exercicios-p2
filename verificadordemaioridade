import java.util.Scanner;

public class Verficadormaioridade {
    public static void main(String[] args) {
        Scanner infouser = new Scanner(System.in);

        String nome = infouser.next();
        byte idade = infouser.nextByte();

        if (idade >= 18) {
            System.out.println("O usuário " + nome + " possui " + idade + " anos logo é maior de idade");
        }else {
            System.out.print("O usuário " + nome + " possui " + idade + " anos logo é menor de idade");
        }
    }
}
